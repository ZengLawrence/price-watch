{
  "name": "Price Watch",
  "description": "Extension to record prices of products on Amazon",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.amazon.com/*"]
    }
  ]
}
